<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.frontend.controllers.user_setting.ChangeEmailController">
    <children>
        <Label layoutX="18.0" layoutY="89.0" text="Current Email:">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </Label>
        <Label layoutX="18.0" layoutY="184.0" text="New Email:">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </Label>
        <Label fx:id="currentEmail" layoutX="260.0" layoutY="80.0">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </Label>
        <TextField fx:id="newEmail" layoutX="260.0" layoutY="175.0">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </TextField>
        <Button fx:id="" layoutX="259.0" layoutY="263.0" mnemonicParsing="false" onAction="#saveEmailHandler" text="Save">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="503.0" layoutY="336.0" mnemonicParsing="false" onAction="#BackHandler" text="Back">
            <font>
                <Font name="Times New Roman" size="28.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
